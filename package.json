    "@angular/forms": "next",
    "@angular/http": "next",
    "@angular/platform-browser": "next",
    "@angular/platform-browser-dynamic": "next",
    "@angular/router": "4.0.0-rc.2",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "ng2-restangular": "^0.1.29",
    "primeng": "^2.0.2",
    "reselect": "^2.5.4",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
